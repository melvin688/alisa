{
  "name": "alisa-cake-ordering-system",
  "version": "1.0.0",
  "private": true,
  "description": "Alisa Cake Coffee Ordering System",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:admin": "cd admin && npm install",
    "install:backend": "cd backend && npm install",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:admin": "cd admin && npm install && npm run build",
    "start:frontend": "cd frontend && npm run dev",
    "start:admin": "cd admin && npm run dev",
    "start:backend": "cd backend && node server.js",
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:admin\" \"npm run start:backend\""
  },
  "repository": {
    "type": "git",
